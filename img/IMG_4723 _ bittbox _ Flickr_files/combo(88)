YUI.add("cookie-banner-view",function(e,i){require("hermes-core/flog")(i);e.FlickrView.create(this.name,e.FlickrView,[],{langBundles:this.details.langBundles,initializer:function(e){this.params=e},loadState:function(){return e.Promise.resolve()},buildContainer:function(){var e="<p>"+this.intlMessage({intlName:"privacy.COOKIE_BANNER_TEXT",url:"/help/cookies"})+"</p>";return this.setContainerWithTemplate("basic-banner",{message:e,additionalClasses:"foot-banner-style"}),this},activate:function(){var e=this.get("container");return this.registerEventHandler(e.one(".button.dismiss").on("click",this.dismissBanner.bind(this))),this.fire("activated"),this},dismissBanner:function(i){var s=new Date;s.setTime(s.getTime()+31536e6),this.appContext.setCookie({cookieName:e.config.flickr.banners.dismiss_cookie_banner_name,cookieValue:1,expires:s,path:"/"}),this.fire("dismiss")}})},"@VERSION@",{requires:["flickr-view","hermes-template-basic-banner","user-limits-helper","person-models","person-profile-models"],langBundles:["privacy"]});